{
  "name": "redux-express-starter",
  "version": "2.0.0",
  "description": "Simple starter package for a full stack web application running React and Express",
  "scripts": {
    "postinstall": "cd client && yarn && yarn build",
    "start": "concurrently \"yarn run server\" \"yarn run client\"",
    "server": "node server/server.js",
    "client": "cd client && yarn start",
    "build": "cd client && yarn build",
    "test": "cd client && react-scripts test --env=jsdom",
    "eject": "cd client && react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rgdalessandro/ReduxExpressStarter.git"
  },
  "author": "Ricardo D'Alessandro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rgdalessandro/ReduxExpressStarter/issues"
  },
  "homepage": "https://github.com/rgdalessandro/ReduxExpressStarter#readme",
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1"
  }
}
